{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Competitors Response Schema",
    "type": "object",
    "required": ["competitors"],
    "properties": {
      "competitors": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "object",
          "required": ["id", "competitor", "differentiating_factor"],
          "properties": {
            "id": {
              "type": "integer",
              "minimum": 1
            },
            "competitor": {
              "type": "string",
              "minLength": 1
            },
            "differentiating_factor": {
              "type": "string",
              "minLength": 1
            }
          },
          "additionalProperties": false
        }
      }
    },
    "additionalProperties": false
  }